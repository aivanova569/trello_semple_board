{
  "name": "trello-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "server:watch": "ts-node-dev --respawn --transpile-only src/index.ts",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migration:generate": "npx sequelize-cli migration:generate --name",
    "seed:generate": "npx sequelize-cli seed:generate --name",
    "seed:up": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "dev": "nodemon --exec ts-node --files -r tsconfig-paths/register ./src/index.ts",
    "prebuild": "npm run lint",
    "lint": "eslint --cache --ext .ts .",
    "lint:fix": "eslint --cache --ext .ts . --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-standard-with-typescript": "^33.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^2.3.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.1",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/validator": "^13.7.10",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2",
    "swagger-ui-express": "^4.6.0",
    "tsconfig-paths": "^4.1.2",
    "tslib": "^2.4.1"
  }
}
